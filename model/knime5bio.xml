<?xml version="1.0" encoding="UTF-8"?>
<plugin package="com.github.lindenb.knime5bio"
	xmlns:xi="http://www.w3.org/2001/XInclude"
	abstract-node-extends-model="com.github.lindenb.knime5bio.AbstractKnime5BioNodeModel"
	>
  <category name="bio">
	<category name="fasta" label="Fasta">
  		<xi:include href="node.read-fasta.xml"/>
  		<xi:include href="node.write-fasta.xml"/>        
    </category>
	<category name="vcf" label="VCF">
  		<category name="jvarkit" label="JVARKIT">
  			<description>Tools from jvarkit</description>
  			<xi:include href="node.jvarkit-filterso.xml"/>
  			<xi:include href="node.jvarkit-vcfhead.xml"/>
  			<xi:include href="node.jvarkit-vcftail.xml"/>
  			<xi:include href="node.jvarkit-vcffilterjs.xml"/>
  			<xi:include href="node.jvarkit-vcfmerge.xml"/>
  			<xi:include href="node.jvarkit-vcfgo.xml"/>
			<xi:include href="node.jvarkit-vcfin.xml"/>
			<xi:include href="node.jvarkit-groupbygene.xml"/>
			<xi:include href="node.jvarkit-vcftrio.xml"/>
  		</category>
  		<xi:include href="node.vcf-samples.xml"/>  
  		<xi:include href="node.vcf2table.xml"/> 
  		<xi:include href="node.vcf-indextabix.xml"/> 
    </category>
  </category>
</plugin>
