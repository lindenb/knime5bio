<?xml version="1.0" encoding="UTF-8"?>
<node name="vcfbigwig" label="VCF BigWig" 
	generate-model='true'
	xmlns:xi="http://www.w3.org/2001/XInclude"
	>

  <description>
     <h3>Motivation</h3>
     <p>See  <a href="https://github.com/lindenb/jvarkit/wiki/VCFBigWig">https://github.com/lindenb/jvarkit/wiki/VCFBigWig</a>. Annotate a VCF with the data of a bigwig file..</p>
     <xi:include href="footer.html" xpointer="xpointer(/div/*)" parse="xml" />
  </description>


	<xi:include href="inport.vcf.xml"/>
	<xi:include href="outport.vcf_count.xml"/>


	<xi:include href="property.vcffile_column.xml"/>
	<property type="string" name="vcftag" label="INFO tag"  default="BIGWIGTAG"/>
	<property type="bool" name="contained" label="BigWig feature must be contained in vcf"  default="true"/>

	<property type="file-read" name="bigWig" >
	  <extension>.bw</extension>
	</property>



	<xi:include href="code.vcfbigwig.xml" parse="xml" />
	<view class="com.github.lindenb.knime5bio.bio.VcfNodeView"/>
</node>

